<div class="row">
    <h2>{{ adGroupTitle }}</h2>
    {%  for entity in ad  %}
        <div class="col-md-2 thumbnail">
            {% if entity.image %}
                <img src="{{ entity.image.getWebPath | imagine_filter('my_thumb_out')  }}" class="img-rounded img-responsive media-object pull-left" />
            {% endif %}
            {{ entity.text|raw('html') }}
        </div>
    {% endfor %}
</div>
